{
    "learning": {
        "batch_size": 128,
        "gamma": 0.99,
        "replay_buffer_size": 10000,
        "num_episodes": 1000,
        "learning_starts": 1000,
        "learning_freq": 8,
        "target_update_freq": 100,
        "log_every": 100,
        "models_path": "models"
    },
    "environment": {
        "track_name": "simple",
        "path": "./race/index.js"
    },
    "target_function": {
        "target_function_name": "ddqn"
    },
    "loss": {
        "loss_name": "huber",
        "loss_kwargs": {
            "delta": 1.0
        }
    },
    "optimizer": {
        "optimizer_name": "rmsprop",
        "optimizer_kwargs": {
            "lr": 0.01,
            "alpha": 0.95, 
            "eps": 0.01
        }
    },
    "eps_scheduler": {
        "eps_scheduler_name": "linear",
        "eps_scheduler_kwargs": {
            "start": 1.0,
            "end": 0.1,
            "num_steps": 10000
        }
    }
}